from all_tables
where table_name in ('SIGNATURE','MAIL_ITEM','MAIL_ITEM_ID','TRACKED_ITEM_BARCODE','PRE_ADV_MNFST_LINE','ERROR','OBJECT_EVENT','OBJECT','PRE_ADVICE_MNFST','EVENTS','IDENTIFYING_MECHANISM')
