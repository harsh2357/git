set serveroutput on
BEGIN
DBMS_SCHEDULER.SET_ATTRIBUTE(NAME => 'IM_BASIC_30',
                             ATTRIBUTE => 'START_DATE',
                             VALUE => '06-OCT-2010 01.00.00.600000 AM EUROPE/LONDON'
