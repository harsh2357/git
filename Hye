		SUBSTR('JC510239377GB',3,8) BETWEEN BARCODE_START_NUM AND BARCODE_END_NUM
       AND   SUBSTR('JC510239377GB',1,2) = BARCODE_PREFIX 
      AND   SUBSTR('JC510239377GB',12,2)   = BARCODE_SUFFIX 
